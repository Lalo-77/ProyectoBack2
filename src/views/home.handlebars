<header>
    <h1> Mis Productos </h1>

    <div>
        <p>Bienvenido, {{user.first_name}} - {{user.last_name}}!! </p>
        <p>E-mail: {{user.email}} </p>
        <p>Edad: {{user.age}} </p>
        <p>Rol: {{user.role}} </p>
      <a href="/api/session/logout" >Cerrar Session</a>
    </div>
</header>
<div class="container"></div>
<h1 class="text-center"></h1>

<div class="row">
    {{#each listadeproductos}}
    <div class="col">
        <div class="card">
            <div class="card-header bg-primary text-white">
                <i class=""></i> Code: {{this.code}}
            </div>
            <div class="card-body">
                <h5 class="card-title text-white">{{this.title}}</h5>
                <ul class="list">
                    <li><i class="bi"></i> ID: {{this.id}}</li>
                    <li><i class="bi"></i> Description: {{this.description}}</li>
                    <li><i class="bi bi-currency-dollar"></i> Price: ${{this.price}}</li>
                    <li><i class="bi bi-grid"></i> Category: {{this.category}}</li>
                    <li><i class=""></i> Status: {{this.status}}</li>
                    <li><i class=""></i> Stock: {{this.stock}}</li>
                    <li>
                        <i class="image"></i> Image: <img src="{{this.thumbnail}}" alt="{{this.title}}" class="img">
                    </li>
                    <br><br>
                    <a href="/api/session/carts" class="carrito">Comprar</a>
                </ul>
            </div>
        </div>
    </div>
    {{/each}}
</div>